<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.letsbiz.salesapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Controller.UserProfile"></activity>
        <activity android:name=".Controller.FeedbackList" />
        <activity android:name=".Controller.HomeScreen" />
        <activity android:name=".Controller.FeedbackDetails" />
        <activity android:name=".Controller.AddFeedback" />
        <activity android:name=".Controller.RegisterActivity" />
        <activity android:name=".Controller.LoginActivity" />
        <activity android:name=".Controller.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>